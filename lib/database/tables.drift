CREATE TABLE Schedule (
    id TEXT PRIMARY KEY NOT NULL,
    jsonString TEXT NOT NULL,
    cachedAt TEXT NOT NULL,
    generatedUuids TEXT NOT NULL
);

CREATE TABLE Preferences (
    viewType INT NOT NULL,
    theme TEXT NOT NULL,
    defaultSchool TEXT NOT NULL,
    preferenceId INT PRIMARY KEY NOT NULL,
    defaultSchedule TEXT NOT NULL,
    notificationTime TEXT NOT NULL
);